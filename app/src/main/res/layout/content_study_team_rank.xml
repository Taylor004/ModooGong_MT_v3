                android:textSize="20dp"
                android:textColor="@color/black"
                android:text="모두 함께 공부합시다"/>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <de.hdodenhof.circleimageview.CircleImageView
                            xmlns:app="http://schemas.android.com/apk/res-auto"
                            android:id="@+id/profile_image1"
                            android:layout_width="96dp"
                            android:layout_height="96dp"
                            android:layout_gravity="start|bottom"
                            android:layout_marginTop="35dp"
                            android:src="@mipmap/ic_app"
                            app:civ_border_width="2dp"
                            app:civ_border_color="#FF000000"/>

                        <ImageButton
                            android:id="@+id/btn_profile_image_editor"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="20dp"
                            android:visibility="gone"
                            android:background="@drawable/btn_circle_white_camera"/>

                        <ImageButton
                            android:id="@+id/btn_profile_Add_Friend"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="10dp"
                            android:layout_marginLeft="32dp"
                            android:visibility="gone"
                            android:background="@drawable/btn_circle_white_pencil"/>

                        <ImageButton
                            android:id="@+id/btn_profile_delete_Friend"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginTop="20dp"
                            android:layout_marginLeft="66dp"
                            android:visibility="gone"
                            android:background="@android:drawable/ic_delete"/>

                    </FrameLayout>


                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <ImageView
                            android:id="@+id/onAirImageView1"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_marginTop="6dp"
                            android:layout_marginLeft="12dp"
                            android:src="@android:color/holo_red_dark" />

                        <TextView
